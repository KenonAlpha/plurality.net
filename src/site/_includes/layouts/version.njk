{%- from 'macros/nav.njk' import renderNav -%}
{% extends "layouts/base.njk" %}

{% block headerScripts %}{% endblock %}

{% block topNav %}
  {{ renderNav(
  leftData = [
    {
      title: translations.return,
      path: "/",
      return: true
    }
  ],
  centreData = [],
  rightData = []
  ) }}
{% endblock %}

{% block mainColContent %}
  {% include "partials/version-content.njk" %}
{% endblock %}

{% block extraneousContent %}{% endblock %}

{% block footer %}{% endblock %}
{% block footerScripts %}
  <script src="{{ site.baseurl }}/assets/js/light-dark-mode.js"></script>
{% endblock %}